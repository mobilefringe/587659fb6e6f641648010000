<script>
    $(document).ready(function(e){
        // init(e);
        loadMallDataCached(renderPage);
    })

    function renderPage(){
        var events = getEventsList();
        console.log(events);
        if(events.length > 0){
            renderEvents("#events_container","#events_template", events);
            $("#events").show();
        } else { 
            $('#no_events').show();
        }
    }
</script>