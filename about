<script>
    $(document).ready(function(e){
        // init(e);
        loadMallDataCached(renderPage);
    })
    
    function renderPage(){
        var prop_details = getPropertyDetails();
        console.log(prop_details);
        renderAddress('#address_container', '#address_template', prop_details);
    }
    
    function renderAddress(container, template, collection){
        var item_list = [];
        var item_rendered = [];
        var template_html = $(template).html();
        Mustache.parse(template_html); 
        
        var repo_rendered = Mustache.render(template_html, collection);
        item_rendered.push(repo_rendered);

        $(container).html(item_rendered.join(''));
    }
</script>